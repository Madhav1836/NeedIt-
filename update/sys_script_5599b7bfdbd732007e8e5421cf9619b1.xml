        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	// Write an info level log message to log the Requested for's email address
	gs.info("Requested for's email is: +current.u_requested_for.email");
	
//Write an info message to the page showing the Requested for's employee number
gs.addInfoMessage("Requested for's employee number is : "+ current.u_requested_for.employee_number);

	
//Write an info message to the page if the current logged user has the admin role
	if(gs.hasRole("admin")){
		
		gs.addInfoMessage("Currently logged user has the admin role");
		
	}
	
	
})(current, previous);]]></script>
